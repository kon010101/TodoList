me = todoLists.map((item) => {\n      return (\n        <span\n          className=\"todo-finished-item\"\n          style={{\n            backgroundColor: item.color,\n          }}\n        >\n          {item.headline}\n        </span>\n      );\n    });\n    return projectName;\n  }\n\n  return (\n    <div className=\"sidebar\">\n      <h1>Important</h1>\n      <div className=\"sidebar-list\">{renderPrioItems()}</div>\n      <h1>Projects</h1>\n      <div className=\"sidebar-list\">{renderListCategories()}</div>\n      <h1>Done</h1>\n      <div className=\"sidebar-list\">{renderDoneItems()}</div>\n    </div>\n  );\n}\n\nexport default Sidebar;\n",{"ruleId":"69","replacedBy":"70"},{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","severity":1,"message":"74","line":1,"column":17,"nodeType":"75","messageId":"76","endLine":1,"endColumn":27},{"ruleId":"73","severity":1,"message":"77","line":9,"column":21,"nodeType":"75","messageId":"76","endLine":9,"endColumn":33},{"ruleId":"73","severity":1,"message":"78","line":2,"column":8,"nodeType":"75","messageId":"76","endLine":2,"endColumn":14},{"ruleId":"73","severity":1,"message":"79","line":9,"column":10,"nodeType":"75","messageId":"76","endLine":9,"endColumn":19},{"ruleId":"73","severity":1,"message":"74","line":1,"column":27,"nodeType":"75","messageId":"76","endLine":1,"endColumn":37},{"ruleId":"73","severity":1,"message":"79","line":14,"column":10,"nodeType":"75","messageId":"76","endLine":14,"endColumn":19},{"ruleId":"73","severity":1,"message":"80","line":19,"column":13,"nodeType":"75","messageId":"76","endLine":19,"endColumn":19},{"ruleId":"73","severity":1,"message":"80","line":46,"column":11,"nodeType":"75","messageId":"76","endLine":46,"endColumn":17},{"ruleId":"73","severity":1,"message":"77","line":8,"column":21,"nodeType":"75","messageId":"76","endLine":8,"endColumn":33},{"ruleId":"81","severity":1,"message":"82","line":21,"column":53,"nodeType":"83","messageId":"84","endLine":21,"endColumn":55},"no-native-reassign