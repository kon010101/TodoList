\n        return (\n          <span\n            className=\"todo-finished-item\"\n            style={{\n              backgroundColor: item.listColor,\n            }}\n          >\n            {item.text}\n          </span>\n        );\n      }\n    });\n    return renderItems;\n  }\n\n  function renderPrioItems() {\n    let allTodos = todoLists.map((list) => list.todos);\n    allTodos = [].concat(...allTodos);\n    const prioItems = allTodos.filter((el) => el.prio);\n    const renderPrio = prioItems.map((item) => {\n      return (\n        <span\n          className=\"todo-finished-item\"\n          style={{\n            backgroundColor: item.listColor,\n          }}\n        >\n          {item.text}\n        </span>\n      );\n    });\n    return renderPrio;\n  }\n\n  function renderListCategories() {\n    const projectName = todoLists.map((item) => {\n      return (\n        <span\n          className=\"todo-finished-item\"\n          style={{\n            backgroundColor: item.color,\n          }}\n        >\n          {item.headline}\n        </span>\n      );\n    });\n    return projectName;\n  }\n\n  return (\n    <div className=\"sidebar\">\n      <h1>Important</h1>\n      <div className=\"sidebar-list\">{renderPrioItems()}</div>\n      <h1>Projects</h1>\n      <div className=\"sidebar-list\">{renderListCategories()}</div>\n      <h1>Done</h1>\n      <div className=\"sidebar-list\">{renderDoneItems()}</div>\n    </div>\n  );\n}\n\nexport default Sidebar;\n",{"ruleId":"65","severity":1,"message":"66","line":1,"column":17,"nodeType":"67","messageId":"68","endLine":1,"endColumn":27},{"ruleId":"65","severity":1,"message":"69","line":9,"column":21,"nodeType":"67","messageId":"68","endLine":9,"endColumn":33},{"ruleId":"65","severity":1,"message":"70","line":2,"column":8,"nodeType":"67","messageId":"68","endLine":2,"endColumn":14},{"ruleId":"65","severity":1,"message":"71","line":9,"column":10,"nodeType":"67","messageId":"68","endLine":9,"endColumn":19},{"ruleId"